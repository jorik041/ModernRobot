    <xs:restriction base="xs:string">
      <xs:enumeration value="Waiting" />
      <xs:enumeration value="Finished" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="CalculationOrderStatus" nillable="true" type="tns:CalculationOrderStatus" />
